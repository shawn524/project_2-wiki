
<div class="page-header">
  <h1><%= @title %><small></small></h1>
</div>

<% @all_revs.reverse.each do |rev| %>
<div class="panel panel-default">
  <div class="panel-body">
    <%= rev["rev_markdown"] %>
  </div>
  <div class="panel-footer">
    <p class="text-muted"><%= rev["rev_user_name"] %> @ <%= rev["rev_created"].slice(0,19) %></p>
  </div>
</div>
<% end %>
